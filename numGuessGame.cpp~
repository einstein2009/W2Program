#include <iostream>
#include <cstdlib>
#include <ctime>

using namespace std;

int main()
{
	int num;
	int guess;
	bool isGuessed;
	int noOfGuesses
	
	srand(time(0));
	num = rand() % 100;
	isGuessed = false;
	
	
	while (00noOfGuesses < 5) && (!isGuessed)
	{
		cout << "Enter an integer greater than or equal to 0 and less than 100: ";
		cin >> guess;
		cout << endl;
		
		noOfGuesses++;
		
		if (guess == num)
		{
			cout << "Winner! You guessed the correct number." << endl;
			isGuessed = true;
		}
		else if (guess < num)
			cout << "Your guess is lower than the number.\nGuess again!" << endl;
		
		else
			cout << "Your guess is higher than the number.\nGuess again!" << endl;
	}
return 0;
}
